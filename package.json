{
  "name": "builder",
  "version": "0.0.1",
  "scripts": {
    "build": "npm run clean && node ./webpack/webpack-builder",
    "build-prod": "npm run env-prod && npm run build",
    "build-dev": "npm run env-dev && npm run build",
    "build-sync": "npm run env-sync && npm run build",
    "server": "npm run server-stop && forever start ./server/server.js",
    "server-prod": "npm run env-prod && npm run server",
    "server-dev": "npm run env-dev && npm run server",
    "server-sync": "npm run env-sync && npm run server && node ./webpack/webpack-server",
    "server-stop": "forever stopall",
    "env-prod": "node ./server/server-env.js production",
    "env-dev": "node ./server/server-env.js development",
    "env-sync": "node ./server/server-env.js sync",
    "dev": "npm run build-sync && npm run server-sync",
    "clean": "rimraf ./build/*"
  },
  "devDependencies": {
    "babel-core": "^5.4.7",
    "babel-loader": "^5.1.3",
    "css-loader": "^0.14.4",
    "cssnext": "^1.5.2",
    "express": "^4.12.4",
    "extract-text-webpack-plugin": "^0.8.1",
    "file-loader": "^0.8.4",
    "forever": "^0.14.1",
    "fs": "0.0.2",
    "html-webpack-plugin": "^1.4.0",
    "ip": "^0.3.3",
    "ng-annotate-webpack-plugin": "^0.1.2",
    "node-libs-browser": "^0.5.2",
    "path": "^0.11.14",
    "postcss-loader": "^0.4.3",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.3.4",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.9.0"
  },
  "dependencies": {
    "angular": "^1.4.0",
    "angular-resource": "^1.4.0",
    "angular-sanitize": "^1.4.0",
    "angular-touch": "^1.4.0",
    "angular-ui-router": "^0.2.15",
    "lodash": "^3.9.3",
    "velocity-animate": "^1.2.2"
  }
}
